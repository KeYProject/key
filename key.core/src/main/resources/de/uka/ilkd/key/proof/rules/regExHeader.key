\sorts {
    RegEx;
}

\functions {
    RegEx regEx(Seq);
    RegEx opt(RegEx);
    RegEx alt(RegEx, RegEx);
    RegEx regExConcat(RegEx, RegEx);

    RegEx repeat(RegEx, int);
    RegEx repeatStar(RegEx);
    RegEx repeatPlus(RegEx);
}

\predicates {
    match(RegEx, Seq);
}
