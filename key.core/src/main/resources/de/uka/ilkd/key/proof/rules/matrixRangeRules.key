        \assumes(wellFormedMatrix(a, heapSV) ==>)

        \find(setMinus(matrixRange(heapSV, a, lRowA, hRowA, x, hColA),
                       matrixRange(heapSV, a, lRowA, hRowA, y, hColA)))
        \sameUpdateLevel
        \replacewith(\if (x >= y)
                     \then (empty)
                     \else ( matrixRange(heapSV, a, lRowA, hRowA, x, y-1)))
        \heuristics(simplify)
