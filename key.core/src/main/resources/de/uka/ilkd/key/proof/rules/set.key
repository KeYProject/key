\sorts {
    /*! @defaultValue(sEmpty) */
    Set;
}

\functions {
    // unique function symbols
    \unique Set sEmpty;

    // other constructors
    Set sSingleton(any);
    Set sUnion(Set, Set);
    Set sIntersect(Set, Set);
    Set sSetMinus(Set, Set);
    Set sInfiniteUnion {true}(Set);

    int sCard(Set);
}

\predicates {
    sElementOf(any, Set);
    sSubset(Set, Set);
    sDisjoint(Set, Set);
}
