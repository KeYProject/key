(autoModeTime "0")

(branch "dummy ID"
(rule "notRight" (formula "1") (newnames "f_base,f_x,v_i"))
(rule "impLeft" (formula "1") (userinteraction))
(branch
    (builtin "One Step Simplification" (formula "1"))
   (rule "closeTrue" (formula "1"))
)
(branch
   (rule "notLeft" (formula "1"))
   (rule "impRight" (formula "1"))
   (rule "allRight" (formula "2") (inst "sk=v_i_0") (userinteraction))
   (rule "powDef" (formula "2") (term "1,0,1") (inst "i=i") (userinteraction))
   (rule "ifthenelse_split" (formula "2") (term "1,0,1") (userinteraction))
   (branch "v_i_0 >= 0 TRUE"
      (rule "logProdIdentity" (formula "3") (term "0,1") (userinteraction))
      (rule "impRight" (formula "4"))
      (rule "replace_known_left" (formula "1") (term "1,0") (ifseqformula "3"))
       (builtin "One Step Simplification" (formula "1") (ifInst ""  (formula "2")) (ifInst ""  (formula "4")))
      (rule "closeFalse" (formula "1"))
   )
   (branch "v_i_0 >= 0 FALSE"
      (rule "impRight" (formula "3"))
      (rule "close" (formula "3") (ifseqformula "1"))
   )
)
)
