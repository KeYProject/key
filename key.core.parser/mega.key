            wd(o) & o != null
        )
        \heuristics(simplify)
    };

    wd_LocSet_AllObjects {

        \find(
            wd(allObjects(f))
        )
        \replacewith(
            wd(f)
        )
        \heuristics(simplify)
    };

    wd_LocSet_FreshLocs {

        \find(
