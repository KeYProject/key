    // this case occurs when translating \num_of
    bsum_positive1 {
        \find(bsum{uSub;}(i0, i1, \if(b) \then(1) \else(0)))
        \sameUpdateLevel
        \varcond(\notFreeIn(uSub, i0),
            \notFreeIn(uSub, i1))
        \add(bsum{uSub;}(i0, i1, \if(b) \then(1) \else(0)) >= 0 ==>)
