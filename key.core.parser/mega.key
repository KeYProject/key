\rules(wdChecks:on) {
    wd_LocSet_AllFields {

        \find(
            wd(allFields(o))
        )
        \varcond(
            \not \isArray(o)
        )
        \replacewith(
            wd(o)
        )
        \heuristics(simplify)
    };

    wd_LocSet_AllFieldsArr {

        \find(
            wd(allFields(o))
        )
        \varcond(
