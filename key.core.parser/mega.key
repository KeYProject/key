    \lemma
    bsum_num_of_lt_max2 {
        \assumes(i2 > i0 & i0 >= 0 ==>)
        \find(\exists uSub; ((uSub >= i0 & uSub < i2) & !phi) ==>)
        \varcond(\notFreeIn(uSub, i0),
            \notFreeIn(uSub, i2))
        \add(bsum{uSub;}(i0, i2, \if(phi) \then(1) \else(0)) < i2 - i0 ==>)
        \heuristics(simplify_enlarging)
    };

    \lemma
    bsum_num_of_lt_max3 {
