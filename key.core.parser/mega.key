            \notFreeIn(uSub2, i0),
            \notFreeIn(uSub1, t2),
            \notFreeIn(uSub2, t1),
            \notFreeIn(uSub1, i1),
            \notFreeIn(uSub2, i1))
        \add( ==> \forall uSub1; ((uSub1 >= i0 & uSub1 < i1) -> t1 <= ({\subst uSub2; uSub1}t2)) & \exists uSub1; ((uSub1 >= i0 & uSub1 < i1) & t1 < ({\subst uSub2; uSub1}t2)))
