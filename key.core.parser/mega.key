        )
        \heuristics(simplify)
    };

    wd_F_Logical_Op_Or {

        \find(
            F((a | b))
        )
        \replacewith(
            F(a) & F(b)
        )
        \heuristics(simplify)
    };

    wd_T_Logical_Op_Imp {

        \find(
            T((a -> b))
        )
        \replacewith(
