        \heuristics(executeIntegerAssignment)
        \displayname "multiplication"
    };

    assignmentMultiplicationBigint2 {
        \find(\modality{#allmodal}{..
                    #loc = #seAny * #seBigint;
                ...}\endmodality (post))
        \replacewith({#loc := mul(#seAny, #seBigint)}
            \modality{#allmodal}{.. ...}\endmodality (post))
        \heuristics(executeIntegerAssignment)
        \displayname "multiplication"
    };
