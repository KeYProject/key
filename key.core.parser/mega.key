    };
}

\rules(programRules:Java, runtimeExceptions:ban) {
    reference_type_cast {
        \find( ==> \modality{#allmodal}{.. #lhs = (#npit) #se; ...}\endmodality (post))
        \varcond(\hasSort(#npit, G), \not \sub(\typeof(#se), G))
