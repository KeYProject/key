        \sameUpdateLevel
        \varcond(\notFreeIn(uSub, i0),
            \notFreeIn(uSub, i1),
            \notFreeIn(uSub, index))
        "Precondition":
            \add( ==> \forall uSub; ((uSub >= i0 & uSub < i1 & uSub != index) -> t >= 0) & i0 <= index & index < i1);
        "Usage":
            \add(({\subst uSub; index}t) <= bsum{uSub;}(i0, i1, t) ==>)
    };

