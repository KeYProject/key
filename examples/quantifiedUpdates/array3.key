\problem {
	\<{ int[] a; }\> (
		! a = null ->
                { a.length := 10 ||
                  \for int i; a[i] := -i}
                \<{ for ( int i = 0; i != 10; ++i )
                      a[i] += i; }\>
                    \forall int i; (i>=0 & i<10 -> a[i] = 0)
        )
}
