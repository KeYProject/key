// demonstrates eager simplification of updates:
// 1) j is removed from update, because not needed
// 2) i is updated by new value before execution

\programVariables {
  int i, j;
}

\problem {
        \<{
            i = 1;
            j = 3;
            i = 2;
          }\> i = 2
}

