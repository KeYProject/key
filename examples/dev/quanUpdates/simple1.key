\programVariables {
	int i;
        int j;
	int k;
}


\schemaVariables {
	\variables int sv;
        \program Variable #pv;
}


\rules {
	test0 { \add ( ==> { \for sv; \if (gt(sv,0)) i := add(sv, 3) } (i = 0) ) };
}


\problem {
	{ \for int x; \if (lt(x,5)) k := x, i := 5, \if (true) j := 3 } i = j
        &
        { \if (1=2) i := 1, \for int y; i := y, i:=5,
          \for (int x; int y) \if (1=1) i := add(x,y) } i=j
}
