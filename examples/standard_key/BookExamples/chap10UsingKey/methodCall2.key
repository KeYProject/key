\javaSource "methodExample/"; // location of class definitions

\programVariables {
  Person p;
}

\problem {
        \forall int x; 
          {p.age:=x} // assign initial value to 'age'
            (   x >= 0 & p != null
             -> \<{
                    p.birthday();
                  }\> p.age >  x)
}
