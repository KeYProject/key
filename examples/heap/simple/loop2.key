\javaSource "src";

\programVariables {
   MyClass self;
   int[] a;
}

\functions {
   Heap heapAtPre;
}


\problem {
   self != null 
   & self.<created> = TRUE
   & a != null
   & a.<created> = TRUE
   & wellFormed(heap) 
   & heap = heapAtPre 
   
   -> \<{self.loop2(a)@MyClass;}\>
   
        \forall int x; (0 <= x & x < a.length -> a[x] = int::select(heapAtPre,self,MyClass::$attr2))
         & \forall Object o; \forall Field f; ((o = a & \exists int i; f = arr(i))  
                                               | any::select(heap,o,f) = any::select(heapAtPre,o,f))
}
