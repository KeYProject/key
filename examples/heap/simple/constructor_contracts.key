\javaSource "src";


\programVariables {
  MyClient self;
  Heap heapAtPre;
  java.lang.Exception exc;
}



\rules {
  Class_invariant_axiom {  
  \schemaVar \term Heap h;  
  \find ( java.lang.Object::<inv>(h, self) )
  \replacewith (
      {heap:=h} (!self.(MyClient::mc) = null
                 & java.lang.Object::<inv>(heap, self.(MyClient::mc))
       		 & disjoint(allFields(self), self.(MyClient::mc).(MyClass::footprint))) 
  )
  \heuristics ( simplify )
  };
}


\problem {
   wellFormed(heap)
-> {heapAtPre:=heap}
      {exc:=null}
        \[{try {
              MyClient M = MyClient.<createObject>();
              M.<init>()@MyClient;
              //M.<initialized>=true;
              self=M;
            }  catch (java.lang.Exception e) {
              exc=e;
            }
          }\] (  (exc = null -> java.lang.Object::<inv>(heap,self))
               & exc = null
               & \forall java.lang.Object o;
                   \forall Field f;
                     (  elementOf(o, f, freshLocs(heapAtPre))
                      | any::select(heap, o, f) = any::select(heapAtPre, o, f)))
}

