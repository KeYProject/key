// Input file for KeY standalone prover version 0.497
\predicates {
  A; B;
}

\heuristicsDecl {
  recursion;
  recursion_test;
}

\schemaVariables {
  \formula b,b0;
}

\rules {
  recursion_inf  { \find(b) \replacewith(b) \heuristics( recursion )} ;
  recursion_test  { \find(b) \add(==>A) \heuristics( recursion_test )} ;
}

\problem {
(A -> B) -> !!(A -> B)

}

