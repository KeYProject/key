\functions {
	int a; int b; int c; int d;
}

\problem {
	a*a + (a+b)*(c+d+1)*(a+2) + b = (a+b+c+d)*(2+c)
	->
   d * b * a
 =   a * a * -2
   - c * a
   - b
   - b * a
   + c * 2
   - c * b
   + c * c
   - c * a * a
   - d * a * 2
   - d * b * 2
   - c * b * a
   + d * c
   - d * a * a
   + d * 2
}


