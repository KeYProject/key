\functions {
	int x; int y;
}

\problem {
        3*x*x*y + 2*x*y + y + 9*x*x + 5*x - 3 = 0
        &
        2*x*x*x*y - x*y - y + 6*x*x*x - 2*x*x - 3*x + 3 = 0
        &
        x*x*x*y + x*x*y + 3*x*x*x + 2*x*x = 0
 ->
	x * x * x * 2 = x * 5 + x * x * 5
}


