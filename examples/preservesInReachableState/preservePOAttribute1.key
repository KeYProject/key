\javaSource "classes";

\programVariables {
    MyClass my;
}

\problem {
    (inReachableState & MyClass.<nextToCreate> >= 1 & my.<created>=TRUE) -> 
      {my.a := MyClass::<get>(MyClass.<nextToCreate>-1)} inReachableState 
}
