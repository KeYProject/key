\predicates {
	\nonRigid NOTHING;
}


\functions {
	java.lang.Throwable[] tr;
	jint[] ir;
	\nonRigid int NOTHING_EITHER;
}


\problem {
(
	{\for (java.lang.Object[] o; int i) \if (o != tr | i != 2) o[i] := null ||
	 ir[11] := 2 ||
	 ir[13] := 5 ||
	 tr[2] := null} NOTHING
<->
	{ir[13] := 5 ||
	 \for (java.lang.Object[] o; int i) o[i] := null ||
	 ir[11] := 2} NOTHING
)
&
(
	{\for (java.lang.Object[] o; int i) \if (o != tr | i != 2) o[i] := null ||
	 ir[11] := 2 ||
	 ir[13] := 5 ||
	 tr[2] := null} NOTHING_EITHER
=
	{ir[13] := 5 ||
	 \for (java.lang.Object[] o; int i) o[i] := null ||
	 ir[11] := 2} NOTHING_EITHER
)
}
