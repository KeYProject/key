\javaSource "source/";

\programVariables {
int a;
int b;
int max;
Demo o;
}


// inv
// xl_0*yl_0 = x*y + z & y>=0 | o.limit@(Demo)=TRUE)

\problem {
 \forall int xl; \forall int yl; \forall int m; (xl >= 0 & yl >= 0 & o!=null ->
   {a:=xl || b:=yl || max:=m}\[{int c = o.mul(a,b,max); }\](c = xl * yl | o.limit@(Demo)=TRUE)
 )
}

