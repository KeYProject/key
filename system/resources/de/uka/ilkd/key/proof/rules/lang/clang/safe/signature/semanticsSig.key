\functions {
	// Representation of unspecified int. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid int unspecifiedInt(int);
	
	// Representation of unspecified value. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid Value unspecifiedValue(int);
	
	// Representation of unspecified object. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid Object unspecifiedObject(int);	
	
	// Representation of indeterminate value. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid Value indeterminateValue(int);	

	// Representation of indeterminate object. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid Object indeterminateObject(int);	
}

\predicates {
	// Representation of unspecified predicate. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid unspecifiedPredicate(int);
	
	// Representation of undefined or undecidable behavior. 
	// Feed fresh skolem symbol as an argument.
	\nonRigid undefinedBehavior(int);	
}