\schemaVariables {
	\term Object schemaObj;
	\term StorageMode schemaStorageMode;	
}

\rules {
	reuseIsValidPtrSaved {
		\find (isValidPtrSaved(schemaObj, schemaStorageMode) ==>)
		\add(schemaObj = null | isValidObj(schemaObj) & schemaStorageMode != ST_TRAP ==>)
	};	

	reuseObjExistsSaved {
		\find (objExistsSaved(schemaObj, schemaStorageMode) ==>)
		\add(isRealObj(schemaObj) & schemaStorageMode != ST_TRAP ==>)
	};
	
	inferIsValidPtrSaved_ObjExistsSaved {
		\assumes (objExistsSaved(schemaObj, schemaStorageMode) ==>)	
		\find (isValidPtrSaved(schemaObj, schemaStorageMode))
		\replacewith(true)
	};		
}