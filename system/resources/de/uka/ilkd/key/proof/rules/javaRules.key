        \sameUpdateLevel
        "Arrays #src and #dest same":
        \replacewith (
	  {#lhs := 0} \modality{#acmpmodal}{..  ...}\endmodality(post))
	\add( \forall iv; (iv >= 0 & iv < #arlength ->
