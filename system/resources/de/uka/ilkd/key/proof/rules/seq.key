  lenOfSeqDef {
	\schemaVar \term int from, to;
        \schemaVar \term any t;
        \schemaVar \variables int uSub, uSub1, uSub2;

   
	\find(seqLen(seqDef{uSub;} (from, to, t)))
	
	\replacewith(\if(from < to)\then((to - from))\else(0))
                     
      	\heuristics(simplify_enlarging)
    };


