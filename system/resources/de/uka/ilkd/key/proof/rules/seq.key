	\schemaVar \term any x;
	\schemaVar \term int idx;	
   
	\find(alpha::seqGet(seqSingleton(x), idx))
	
	\replacewith(\if(idx = 0)
                     \then( (alpha)x )
                     \else(  (alpha)seqGetOutside ))
                     
      	\heuristics(simplify)
    };
    

