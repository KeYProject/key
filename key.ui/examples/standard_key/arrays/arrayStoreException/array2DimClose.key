}


\javaSource "./classes";

\programVariables {
  CSub12[][] a;
}

\problem {  
     ((!(a=null) & a.length=10 & 
	!(a[0]=null | a[1]=null | a[2]=null) & a[0].length = 4 & a[1].length = 4 & a[2].length = 4) ->  
		\<{ for (int j = 0; j<3; j++) { 
		       for (int i = 0; i<3; i++) { 
		          a[i][j] = a[0][0]; } } }\> true)
}
