// inv  rl = r + q * d


\problem {
\<{
   int r,d,q;
 }\> \forall int rl;
      \forall int dl;
        (   rl >= 0 & dl >= 0
         -> {r:=rl ||
              d:=dl}
                \<{
                   q=0;
                   while ( r>=d ) {
                     r=r-d;
                     q++;
                   }
                 }\> (rl = r + q * d  & r < d))

}



