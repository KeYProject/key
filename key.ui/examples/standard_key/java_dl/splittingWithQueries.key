\javaSource "query_java/";

\programVariables {
	A o;
}

\predicates {
	p; q; r;
}

\problem {
   o != null
->
   o.getX() >= 0
->
   (((p -> q) & (!p -> r)) <-> ((p & q) | (!p & r)))
}
