\problem { wellFormed(heap) ->
\<{
   i =0;
     try {
       throw new  NullPointerException ();
     i=i+1;
   }  catch (RuntimeException e1) {
     i=i+4;
   }  finally {
     i=i+8;
   }
 }\> i = 12

