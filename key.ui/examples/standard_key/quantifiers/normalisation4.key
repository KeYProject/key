\predicates {
	p(int);
	ff;
}

\rules {
	ffEqFalse {
	    \find(ff ==>)
	    \replacewith(false ==>)
	    \heuristics(concrete)
	};
}

// it is important for this test that [StrategyProperty]SPLITTING_OPTIONS_KEY is set to SPLITTING_OFF
\problem {
	\exists int i; (p(i+1) & i < 5 & i != 20 & i != 5 & i < 10 & i != 4 & i > 3 & p(i)) <-> ff
}
