      <extension
            point="org.eclipse.ui.handlers">
         <handler
               class="org.key_project.sed.key.ui.command.SaveProofHandler"
               commandId="org.key_project.sed.key.ui.saveProofCommand">
            <enabledWhen>
                  <or>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <and>
                        <instanceof
                              value="org.eclipse.debug.core.ILaunch">
                        </instanceof>
                        <test
                              args="notTerminated"
                              forcePluginActivation="true"
                              property="org.key_project.sed.key.ui.isSingleKeyDebugTarget">
                        </test>
                     </and>
                     <and>
                        <instanceof
                              value="org.eclipse.debug.core.model.IDebugElement">
                        </instanceof>
                        <test
                              args="notTerminated"
                              forcePluginActivation="true"
                              property="org.key_project.sed.key.ui.isSingleKeyDebugTarget">
                        </test>
                     </and>
                  </or>
               </iterate>
               <test
                     forcePluginActivation="true"
                     property="org.key_project.sed.key.ui.proofViewHasDebugTarget">
               </test>
                  </or>
            </enabledWhen>
         </handler>
         <handler
               class="org.key_project.sed.key.ui.visualization.execution_tree.command.VisualizeMemoryLayoutsCommand"
               commandId="org.key_project.sed.key.ui.visualizeMemoryLayoutsCommand">
            <enabledWhen>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <and>
                        <instanceof
                              value="org.key_project.sed.key.core.model.IKeYSENode">
                        </instanceof>
                        <test
                              forcePluginActivation="true"
                              property="org.key_project.sed.key.ui.canVisualizeMemoryLayouts">
                        </test>
                     </and>
                  </or>
               </iterate>
            </enabledWhen>
         </handler>
         <handler
               class="org.key_project.sed.key.ui.command.OpenProofHandler"
               commandId="org.key_project.sed.key.ui.openProofCommand">
            <enabledWhen>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <and>
                        <instanceof
                              value="org.eclipse.debug.core.ILaunch">
                        </instanceof>
                        <test
                              args="notTerminated, notKeYMainWindow"
                              forcePluginActivation="true"
                              property="org.key_project.sed.key.ui.isSingleKeyDebugTarget">
                        </test>
                     </and>
                     <and>
                        <instanceof
                              value="org.eclipse.debug.core.model.IDebugElement">
                        </instanceof>
                        <test
                              args="notTerminated, notKeYMainWindow"
                              forcePluginActivation="true"
                              property="org.key_project.sed.key.ui.isSingleKeyDebugTarget">
                        </test>
                     </and>
                  </or>
               </iterate>
            </enabledWhen>
         </handler>
         <handler
               class="org.key_project.sed.key.ui.command.AddWatchpointHandler"
               commandId="org.key_project.sed.key.ui.addWatchpointCommand">
         </handler>
         <handler
               class="org.key_project.keyide.ui.handlers.ApplyRuleHandler"
               commandId="org.key_project.sed.key.ui.commands.applyRule">
         </handler>
         <handler
               class="org.key_project.sed.key.ui.command.StartAutoModeHandlerSED"
               commandId="org.key_project.sed.key.ui.startAutoModeCommand">
            <enabledWhen>
               <and>
                  <test
                        forcePluginActivation="true"
                        property="org.key_project.sed.key.ui.isNotAutoMode"
                        value="true">
                  </test>
                  <test
                        forcePluginActivation="true"
                        property="org.key_project.sed.key.ui.proofIsNotClosed"
                        value="true">
                  </test>
               </and>
            </enabledWhen>
         </handler>
         <handler
               class="org.key_project.sed.key.ui.command.StopAutoModeHandlerSED"
               commandId="org.key_project.sed.key.ui.stopAutoModeCommand">
            <enabledWhen>
               <test
                     forcePluginActivation="true"
                     property="org.key_project.sed.key.ui.isAutoMode"
                     value="true">
               </test>
            </enabledWhen>
         </handler>
         <handler
               class="org.key_project.keyide.ui.handlers.GenerateTestCasesHandler"
               commandId="org.key_project.sed.key.ui.generateTestCases">
            <enabledWhen>
               <test
                     forcePluginActivation="true"
                     property="org.key_project.sed.key.ui.isNotAutoMode"
                     value="true">
               </test>
            </enabledWhen>
         </handler>
         <handler
               class="org.key_project.sed.key.ui.ShowSubtreeOfNodeHandler"
               commandId="org.key_project.sed.key.ui.showSubtreeCommand">
         </handler>
