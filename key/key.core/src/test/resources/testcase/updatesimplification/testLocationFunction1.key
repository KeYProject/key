\functions {
   \nonRigid[Location] int c;
   \nonRigid[Location] int d;
   \nonRigid[Location] int f(int);
   \nonRigid[Location] int g(int);      
}

\problem {
   ( {c:=1}(c = 1 & d = 1 & f(c) = 1 & f(d) = 1 & g(c) = 5)) <-> 
       (1 = 1 & d = 1 & f(1) = 1 & f(d) = 1 &  g(1) = 5)
}
