\rules(programRules:Java, dependencies:on) {
  // Unwinding rules
   loopUnwind    {
            \schemaVar \program Statement #s;
            \schemaVar \program Expression #e;
            \schemaVar \formula post;
            \schemaVar \program Label #innerLabel, #outerLabel;


            \find (\modality{#allmodal}{.. while(#e) #s ...}\endmodality(post))
             \varcond(\newLabel(#innerLabel), \newLabel(#outerLabel))
                 \replacewith ({timestamp:=timestamp + 1}{\event(start, empty, timestamp)}\modality{#allmodal}{.. #unwind-loop (#innerLabel, #outerLabel,
                                       while(#e) #s); ...}\endmodality(post))
             \heuristics(loop_expand)
          };

   unwindLoopScope {
      \schemaVar \modalOperator { diamond, box, diamond_transaction, box_transaction } #allmodal;
      \schemaVar \modalOperator { diamond, diamond_transaction } #dia;
      \schemaVar \modalOperator { box, box_transaction } #box;
      \schemaVar \formula post;

      \schemaVar \program SimpleExpression #se;
      \schemaVar \program LeftHandSide #lhs;
      \schemaVar \program Statement #body;
      \schemaVar \program[list] Statement #slist;
      \schemaVar \program Expression #nse;
      \schemaVar \program Label #label, #label1;

      \schemaVar \program Variable #x;


       \find ( (\modality{#allmodal} {.. while (#nse) #body ... }\endmodality(post)) )

       \varcond(\new(#x, boolean))

       \replacewith (
         ({timestamp:=timestamp + 1}{\event(start, empty, timestamp)}\modality{#allmodal} {..
             boolean #x;
             loop-scope(#x) {
               if (#nse) { #body continue; }
               else { break; }
             }
          ... }{\event(end, empty, timestamp)}
           \endmodality(
             (#x<<loopScopeIndex>> = TRUE -> post) &
             (#x<<loopScopeIndex>> = FALSE ->
                (\modality{#allmodal} {..
                    #reattachLoopInvariant(while (#nse) #body);
                 ... }\endmodality(post)))
           )
         )
       )

       \heuristics(loop_scope_expand)
     };

lsContinue {

     \schemaVar \modalOperator { diamond, box, diamond_transaction, box_transaction } #allmodal;
      \schemaVar \modalOperator { diamond, diamond_transaction } #dia;
      \schemaVar \modalOperator { box, box_transaction } #box;
      \schemaVar \formula post;

      \schemaVar \program SimpleExpression #se;
      \schemaVar \program LeftHandSide #lhs;
      \schemaVar \program Statement #body;
      \schemaVar \program[list] Statement #slist;
      \schemaVar \program Expression #nse;
      \schemaVar \program Label #label, #label1;

      \schemaVar \program Variable #x;

      \find        (\modality{#allmodal}{.. loop-scope(#lhs) { continue; #slist } ...}\endmodality(post))
      \replacewith ({#lhs := FALSE}{\event(end, empty, timestamp)} post)
      \heuristics(simplify_prog)
  };

  }



