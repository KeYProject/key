		"Index Out of Bounds (#v0 != null, but #se Out of Bounds!)":
		\replacewith(==> false)
		\add (!(#v0=null) & (leq(length(#v0), #se) | lt(#se,0)) ==>)

		\heuristics(simplify_prog, simplify_prog_subset)
