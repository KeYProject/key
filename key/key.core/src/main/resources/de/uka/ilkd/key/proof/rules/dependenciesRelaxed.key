//======================================================================================================================
//													Splitting Rules for Relaxed
//======================================================================================================================
	//---------------------------------------------------> Dependence Predicates:
/* It could be tricky as they have 3 extra locsets*/
	//---------------------------------------------------> History Predicates:
	splitRelaxedNoRaWAtHistory {
	    \schemaVar \term LocSet loc1, loc2;
	    \schemaVar \term int label;

	    \find(relaxedNoRaWAtHistory(union(loc1,loc2), label))
	    \replacewith(relaxedNoRaWAtHistory(loc1, label) & relaxedNoRaWAtHistory(loc2, label))

	    \heuristics(simplify)
	};

	splitRelaxedNoWaRAtHistory {
	    \schemaVar \term LocSet loc1, loc2;
	    \schemaVar \term int label;

	    \find(relaxedNoWaRAtHistory(union(loc1,loc2), label))
	    \replacewith(relaxedNoWaRAtHistory(loc1, label) & relaxedNoWaRAtHistory(loc2, label))

	    \heuristics(simplify)
	};

	splitRelaxedNoWaWAtHistory {
	    \schemaVar \term LocSet loc1, loc2;
