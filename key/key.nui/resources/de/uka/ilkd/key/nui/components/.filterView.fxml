<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox fx:id="filterViewPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	HBox.hgrow="ALWAYS">
	
   <children>
      <TextField fx:id="tfFilterQuery" promptText="filter term"
      	layoutY="3.0" minHeight="39.0" minWidth="100.0" 
      	AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" 
     	HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" />
      <ToggleButton fx:id="btnFilterActve" text="" textAlignment="CENTER" 
      	minHeight="39.0" minWidth="39.0" AnchorPane.bottomAnchor="0.0"
      	AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Button fx:id="btnFilterClose" text="" textAlignment="CENTER" 
      	minHeight="39.0" minWidth="39.0" AnchorPane.bottomAnchor="0.0"
      	AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
   </children>
</HBox>