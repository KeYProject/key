<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Menu?>
<?import com.sun.javafx.scene.control.skin.ContextMenuContent?>

<BorderPane fx:id="root" prefHeight="600.0" prefWidth="800.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.uka.ilkd.key.nui.NUIController">
	<top>
		<MenuBar BorderPane.alignment="CENTER">
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#handleCloseWindow"
							text="Close" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Edit">
					<items>
						<MenuItem mnemonicParsing="false" text="Delete" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="View">
					<items>
						<Menu mnemonicParsing="false" text="Add View">
							<items>
								<MenuItem mnemonicParsing="false" text="%proof" />
								<MenuItem mnemonicParsing="false" onAction="#handleLoadGoals"
									text="%goals" />
								<MenuItem mnemonicParsing="false" onAction="#handleLoadUserConstraint"
									text="%userConstraint" />
								<MenuItem mnemonicParsing="false"
									onAction="#handleLoadProofSearchStrategy" text="%proofSearchStrategy" />
								<MenuItem mnemonicParsing="false" onAction="#handleLoadRules"
									text="%rules" />
							</items>
						</Menu>
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="%about">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#handleAboutWindow"
							text="%aboutKey" />
						<MenuItem mnemonicParsing="false" text="%license" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
	</top>
	<center>
		<SplitPane dividerPositions="0.25, 0.75" prefHeight="160.0"
			prefWidth="200.0" BorderPane.alignment="CENTER">
			<items>
				<AnchorPane prefHeight="552.0" prefWidth="266.0">
					<children>
						<VBox fx:id="left" prefHeight="200.0" prefWidth="100.0">
						</VBox>
					</children>
				</AnchorPane>
				<AnchorPane fx:id="middle" minHeight="0.0" minWidth="0.0"
					prefHeight="160.0" prefWidth="100.0" />
				<AnchorPane prefHeight="200.0" prefWidth="200.0">
					<children>
						<VBox fx:id="right" prefHeight="200.0" prefWidth="100.0" />
					</children>
				</AnchorPane>
			</items>
			<contextMenu>
				<ContextMenu>
					<items>
						<Menu fx:id="setViews" text="%configViews">
							<items>
								<MenuItem text="%proof">
									<graphic>
										<MenuButton fx:id="ButtonProof" mnemonicParsing="false"
											text="%hide">
											<items>
												<MenuItem fx:id="toLeft" mnemonicParsing="false"
													text="%left" onAction="#handleLoadComponent">
													<properties componentName="proof" />
												</MenuItem>
												<MenuItem fx:id="toMiddle" mnemonicParsing="false"
													text="%middle" onAction="#handleLoadComponent">
													<properties componentName="proof" />
												</MenuItem>
												<MenuItem fx:id="toRight" mnemonicParsing="false"
													text="%right" onAction="#handleLoadComponent">
													<properties componentName="proof" />
												</MenuItem>
												<MenuItem fx:id="hide" mnemonicParsing="false"
													text="%hide" onAction="#handleLoadComponent">
													<properties componentName="proof" />
												</MenuItem>
											</items>
										</MenuButton>
									</graphic>
								</MenuItem>
								<MenuItem text="%goals">
									<graphic>
										<MenuButton fx:id="ButtonGoals" mnemonicParsing="false"
											text="%hide">
											<items>
												<MenuItem fx:id="toLeft" mnemonicParsing="false"
													text="%left" onAction="#handleLoadComponent">
													<properties componentName="goals" />
												</MenuItem>
												<MenuItem fx:id="toMiddle" mnemonicParsing="false"
													text="%middle" onAction="#handleLoadComponent">
													<properties componentName="goals" />
												</MenuItem>
												<MenuItem fx:id="toRight" mnemonicParsing="false"
													text="%right" onAction="#handleLoadComponent">
													<properties componentName="goals" />
												</MenuItem>
												<MenuItem fx:id="hide" mnemonicParsing="false"
													text="%hide" onAction="#handleLoadComponent">
													<properties componentName="goals" />
												</MenuItem>
											</items>
										</MenuButton>
									</graphic>
								</MenuItem>
								<MenuItem text="%userConstraint">
									<graphic>
										<MenuButton fx:id="ButtonUserConstraint"
											mnemonicParsing="false" text="%hide">
											<items>
												<MenuItem fx:id="toLeft" mnemonicParsing="false"
													text="%left" onAction="#handleLoadComponent">
													<properties componentName="userConstraint" />
												</MenuItem>
												<MenuItem fx:id="toMiddle" mnemonicParsing="false"
													text="%middle" onAction="#handleLoadComponent">
													<properties componentName="userConstraint" />
												</MenuItem>
												<MenuItem fx:id="toRight" mnemonicParsing="false"
													text="%right" onAction="#handleLoadComponent">
													<properties componentName="userConstraint" />
												</MenuItem>
												<MenuItem fx:id="hide" mnemonicParsing="false"
													text="%hide" onAction="#handleLoadComponent">
													<properties componentName="userConstraint" />
												</MenuItem>
											</items>
										</MenuButton>
									</graphic>
								</MenuItem>
								<MenuItem text="%proofSearchStrategy">
									<graphic>
										<MenuButton fx:id="ButtonProofSearchStrategy"
											mnemonicParsing="false" text="%hide">
											<items>
												<MenuItem fx:id="toLeft" mnemonicParsing="false"
													text="%left" onAction="#handleLoadComponent">
													<properties componentName="proofSearchStrategy" />
												</MenuItem>
												<MenuItem fx:id="toMiddle" mnemonicParsing="false"
													text="%middle" onAction="#handleLoadComponent">
													<properties componentName="proofSearchStrategy" />
												</MenuItem>
												<MenuItem fx:id="toRight" mnemonicParsing="false"
													text="%right" onAction="#handleLoadComponent">
													<properties componentName="proofSearchStrategy" />
												</MenuItem>
												<MenuItem fx:id="hide" mnemonicParsing="false"
													text="%hide" onAction="#handleLoadComponent">
													<properties componentName="proofSearchStrategy" />
												</MenuItem>
											</items>
										</MenuButton>
									</graphic>
								</MenuItem>
								<MenuItem text="%rules">
									<graphic>
										<MenuButton fx:id="ButtonRules" mnemonicParsing="false"
											text="%hide">
											<items>
												<MenuItem fx:id="toLeft" mnemonicParsing="false"
													text="%left" onAction="#handleLoadComponent">
													<properties componentName="rules" />
												</MenuItem>
												<MenuItem fx:id="toMiddle" mnemonicParsing="false"
													text="%middle" onAction="#handleLoadComponent">
													<properties componentName="rules" />
												</MenuItem>
												<MenuItem fx:id="toRight" mnemonicParsing="false"
													text="%right" onAction="#handleLoadComponent">
													<properties componentName="rules" />
												</MenuItem>
												<MenuItem fx:id="hide" mnemonicParsing="false"
													text="%hide" onAction="#handleLoadComponent">
													<properties componentName="rules" />
												</MenuItem>
											</items>
										</MenuButton>
									</graphic>
								</MenuItem>
							</items>
						</Menu>
					</items>
				</ContextMenu>
			</contextMenu>
		</SplitPane>
	</center>
	<bottom>
		<ToolBar prefHeight="21.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<items>
				<Label fx:id="statustext" prefHeight="17.0" prefWidth="334.0"
					text="%status" />
			</items>
		</ToolBar>
	</bottom>
</BorderPane>

